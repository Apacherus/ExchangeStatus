{
  "name": "Download Selected Links",
  "description": "Select links on a page and download them.",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["jquery-2.1.4.min.js", "background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "page_action": {
    "default_icon": {
      "19": "img/alert-icon.png",
      "38": "img/alert-icon.png"
    },
    "default_title": "Exchange Status Notification",
    "default_popup": "page-action.html"
  }
}
